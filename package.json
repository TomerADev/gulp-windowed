    "through2": "^3.0.1",
    "vinyl": "^2.2.0"
  },
  "devDependencies": {
    "@ava/babel": "^1.0.1",
    "@babel/core": "^7.9.6",
    "@babel/register": "^7.9.0",
    "ava": "^3.8.2",
    "babel-eslint": "10.1.0",
    "core-js": "^3.6.5",
    "eslint": "7.0.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-ava": "10.3.0",
    "eslint-plugin-prettier": "3.1.3",
    "get-stream": "^5.1.0",
    "microbundle": "^0.12.0",
    "npm-run-all": "^4.1.5",
    "prettier": "2.0.5",
    "regenerator-runtime": "^0.13.5",
    "stream-assert": "^2.0.3"
